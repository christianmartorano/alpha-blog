<h1 class="center">Listing all Categories</h1>
<div class="center">
  <%= will_paginate %>
    <% @categories.each do |category| %>
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <ul class="listing">
            <li class="article-title"><%= link_to "#{category.name}", category_path(category) %></li>
          </ul>
        </div>
      </div>
    <% end %>
  <%= will_paginate %>
</div>